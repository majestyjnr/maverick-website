                        <div class="col-md-9">
                            <form onsubmit="return subscribe(this)" method="POST">
                                <div class="row">
                                    <div class="col-md-4">
                                        <input type="text" id="name" name="name" placeholder="Name" required />
                                    </div>
                                    <div class="col-md-4">
